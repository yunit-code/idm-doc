#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_vuepress@2.0.0-beta.33@vuepress/bin/vuepress.js" "$@"
  ret=$?
else 
  node  "$basedir/../_vuepress@2.0.0-beta.33@vuepress/bin/vuepress.js" "$@"
  ret=$?
fi
exit $ret
